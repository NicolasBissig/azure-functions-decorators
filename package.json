{
  "version": "0.1.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "tsdx": "tsdx",
    "start": "tsdx watch",
    "build": "tsdx build --target es2020 --tsconfig tsconfig.build.json",
    "pretest": "npm run build",
    "test": "jest",
    "test:mutation": "stryker run",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "azure-functions-decorators",
  "author": "Nicolas Bissig",
  "homepage": "https://github.com/NicolasBissig/azure-functions-decorators",
  "repository": {
    "type": "git",
    "url": "https://github.com/NicolasBissig/azure-functions-decorators"
  },
  "module": "dist/azure-functions-decorators.esm.js",
  "size-limit": [
    {
      "path": "dist/azure-functions-decorators.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/azure-functions-decorators.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@azure/functions": "3.2.0",
    "@size-limit/preset-small-lib": "8.1.0",
    "@stryker-mutator/core": "6.3.0",
    "@stryker-mutator/jest-runner": "6.3.0",
    "@types/jest": "29.2.3",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "size-limit": "8.1.0",
    "ts-jest": "29.0.3",
    "tsdx": "0.14.1",
    "tslib": "2.4.1",
    "typescript": "4.9.3"
  }
}
