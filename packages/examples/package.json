{
    "name": "azure-functions-decorators-examples",
    "version": "1.0.0",
    "private": true,
    "description": "",
    "scripts": {
        "build": "tsc --project tsconfig.build.json",
        "watch": "tsc --project tsconfig.build.json -w",
        "prestart": "npm run build",
        "start": "npm run build && npm run start:host",
        "start:host": "npx func start",
        "start:watch": "npm-run-all --parallel start:host watch",
        "test": "concurrently --kill-others --names server,jest --success first \"npm run start\" \"npx wait-on http-get://localhost:7071/api/health --timeout 60000 && npm run test:only\"",
        "test:only": "jest"
    },
    "devDependencies": {
        "azure-functions-core-tools": "4.0.4915",
        "npm-run-all": "4.1.5",
        "@types/node-fetch": "2.6.2",
        "node-fetch": "2.6.7",
        "wait-on": "7.0.1",
        "concurrently": "7.6.0",
        "azure-functions-decorators": "*"
    }
}
